{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "commit": "git status && git add -A && git-cz",
    "prepare": "husky"
  },
  "dependencies": {
    "axios": "1.11.0",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.5.0",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@eslint/js": "9.33.0",
    "@types/node": "24.3.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/tsconfig": "^0.7.0",
    "commitizen": "4.3.1",
    "cz-git": "1.12.0",
    "eslint": "9.33.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-vue": "10.4.0",
    "globals": "16.3.0",
    "husky": "9.1.7",
    "lint-staged": "16.1.5",
    "prettier": "3.6.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "8.39.1",
    "unplugin-auto-import": "20.0.0",
    "unplugin-vue-components": "29.0.0",
    "vite": "^7.0.0",
    "vue-tsc": "^3.0.5"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{json,md,css,scss}": [
      "prettier --write"
    ]
  }
}
